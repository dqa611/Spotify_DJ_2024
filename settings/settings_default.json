{
    "models": {
        "common_text_score": {
            "applied": true,
            "desc": "Common Genre",
            "input": {
                "default_input_index_score": 100,
                "min_cap_score": 1,
                "partial_score_multiplier": 0.5,
                "percentile_ascending": false,
                "return_percentile": 1,
                "split_str": "|",
                "which_col": "artist_genres"
            }
        },
        "harmonic_keys": {
            "applied": true,
            "desc": "Harmonic",
            "input": {
                "applied": [
                    "Same scale",
                    "Tonal shift",
                    "Up a fifth",
                    "Down a fifth",
                    "Going down minor third",
                    "Minor to Major",
                    "Parallel Key",
                    "Up Dominant Key of the Relative Major",
                    "Down Dominant Key of the Relative Major",
                    "Up whole step",
                    "Down whole step",
                    "Up half step"
                ],
                "harmonic_rules": {
                    "Same scale": {
                        "key_mvmt": 0,
                        "mode_mvmt": 0,
                        "camelot_mvmt": 0,
                        "example": "C to C"
                    },
                    "Tonal shift": {
                        "key_mvmt": -3,
                        "mode_mvmt": 1,
                        "camelot_mvmt": 0,
                        "example": "C to Am"
                    },
                    "Up a fifth": {
                        "key_mvmt": 7,
                        "mode_mvmt": 0,
                        "camelot_mvmt": 1,
                        "example": "C to G"
                    },
                    "Down a fifth": {
                        "key_mvmt": 5,
                        "mode_mvmt": 0,
                        "camelot_mvmt": -1,
                        "example": "C to F"
                    },
                    "Going down minor third": {
                        "key_mvmt": 9,
                        "mode_mvmt": 0,
                        "camelot_mvmt": 3,
                        "example": "C to A"
                    },
                    "Minor to Major": {
                        "key_mvmt": 6,
                        "mode_mvmt": 1,
                        "camelot_mvmt": 3,
                        "example": "C to F#m"
                    },
                    "Parallel Key": {
                        "key_mvmt": 0,
                        "mode_mvmt": 1,
                        "camelot_mvmt": -3,
                        "example": "C to Cm"
                    },
                    "Up Dominant Key of the Relative Major": {
                        "key_mvmt": 4,
                        "mode_mvmt": 1,
                        "camelot_mvmt": 1,
                        "example": "C to Em"
                    },
                    "Down Dominant Key of the Relative Major": {
                        "key_mvmt": 2,
                        "mode_mvmt": 1,
                        "camelot_mvmt": -1,
                        "example": "C to Dm"
                    },
                    "Up whole step": {
                        "key_mvmt": 2,
                        "mode_mvmt": 0,
                        "camelot_mvmt": 2,
                        "example": "C to D"
                    },
                    "Down whole step": {
                        "key_mvmt": -2,
                        "mode_mvmt": 0,
                        "camelot_mvmt": -2,
                        "example": "C to Bb aka A#"
                    },
                    "Up half step": {
                        "key_mvmt": 1,
                        "mode_mvmt": 0,
                        "camelot_mvmt": 7,
                        "example": "C to C#"
                    }
                },
                "energy_interpretation": {
                    "0": "Perfectly harmonic",
                    "1": "Raise energy",
                    "-1": "Goosebumps"
                },
                "mood_interpretation": {
                    "0": "Same mood",
                    "1": "Brighten mood",
                    "-1": "Darken mood"
                }
            }
        },
        "nearest_neighbor": {
            "applied": true,
            "desc": "Similarity",
            "input": {
                "max_cap_score": 3,
                "nn_metrics": [
                    "year_released",
                    "danceability",
                    "energy",
                    "acousticness",
                    "valence",
                    "tempo"
                ],
                "nn_number_of_output": 10,
                "percentile_ascending": true,
                "return_percentile": 0.5
            }
        }
    },
    "paths": {
        "default_data": "./data/spotify_playlist_data.csv",
        "default_input": "./spotify_playlist_ids.csv",
        "default_path": "./settings_default.json",
        "manual_input": "./settings_manual.json"
    },
    "preferences": {
        "length_min": 30,
        "preferred_order": {
            "applied": true,
            "order": [
                "common_text_score|nearest_neighbor",
                "harmonic_keys|nearest_neighbor",
                "common_text_score|harmonic_keys"
            ]
        },
        "randomize": {
            "applied": 0,
            "options": {
                "Optimal": 0,
                "Random": 1,
                "Weighted": [
                    0.5,
                    0.2,
                    0.1
                ]
            }
        }
    },
    "import_from_spotify": false,
    "export_to_csv": true,
    "export_to_spotify": true,
    "print_log": true,
    "print_plot": true
}